{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://raw.githubusercontent.com/entityschema/jschema/main/spec/website.json",
    "title": "Website",
    "description": "Website is page available on internet with specific URL.",
    "type": "object",
    "properties": {
      "url": {
        "description": "URL of webpage. Usualy domain name.",
        "type": "string",
        "format": "uri",
        "pattern": "^(https?|ftp)://[^\\s/$.?#].[^\\s]*$"
      }
    },
    "required": [ "url" ],
    "examples": [
        {
            "url": "https://www.honzapospisil.com"
        }
    ]
  }
  